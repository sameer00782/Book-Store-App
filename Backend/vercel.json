{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json", 
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "Backend/package.json",    
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/index.js" },
      { "src": "/(.*)", "dest": "/frontend/index.html" }
    ]
  }
  